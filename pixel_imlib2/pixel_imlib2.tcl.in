package require Pixel 3

package require Itcl 3.1
namespace import -force ::itcl::*

load [file join $::pixel::imlib2::library @PKG_LIB_FILE@]

namespace eval ::pixel::imlib2 {
	namespace export *

	variable version @PACKAGE_VERSION@.@EXTRA_VERSION@
}

package provide @PACKAGE_NAME@ $::pixel::imlib2::version

